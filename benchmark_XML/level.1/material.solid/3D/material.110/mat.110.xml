<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="../geometry/cube.1.geom"
    title="3D uniaxial extension" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="3" node_ID="2" schedule="1" type="u" value="0.01"/>
            <kinematic_BC dof="2" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="4"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                strain_energy_density="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <SIERRA_hypoelastic
                        SIERRA_parameter_file="mat.110.params" density="1.0">
                        <bulk_and_shear bulk_modulus="15177.0" shear_modulus="9001.5"/>
                    </SIERRA_hypoelastic>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+01" max_iterations="3" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
