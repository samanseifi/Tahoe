<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="../geometry/square.1.geom"
    title="2D uniaxial tension" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="2" schedule="1" type="u" value="0.01"/>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="4"/>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_Hookean_2D density="1.0">
                        <modulus_3x3 C_1_1="30.0" C_2_1="10.0"
                            C_2_2="30.0" C_3_3="10.0" copy_symmetric="true"/>
                    </small_strain_Hookean_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+01" max_iterations="10" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
