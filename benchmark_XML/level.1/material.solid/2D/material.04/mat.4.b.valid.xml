<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
              title='2D uniaxial tension'
             author='paklein'
    geometry_format='automatic'
      geometry_file='../geometry/square.1.geom'
      output_format='automatic'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	       scaling='linear'
	     init_time='0.000000e+00'
	     num_steps='2'
	    output_inc='1'
	 max_step_cuts='0'
	     time_step='5.000000e-01'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='0.000000e+00'/>
				<OrderedPair
				 x='1.000000e+00'
				 y='1.000000e+00'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='displacement'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='2'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='2'
			      dof='2'
			     type='u'
			 schedule='1'
			    value='1.000000e-02'/>
			<kinematic_BC
			  node_ID='3'
			      dof='1'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='4'
			      dof='1'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
		</field>
	</nodes>
	<element_list>
		<updated_lagrangian
		     field_name='displacement'
		      mass_type='automatic'
		 eigenvalue_inc='0'>
			<quadrilateral
			 num_ip='4'/>
			<solid_element_nodal_output
			   displacements='1'
			          stress='1'
			 material_output='1'/>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<large_strain_material_2D>
					<large_strain_cubic_2D
					            density='1.000000e+00'
					      constraint_2D='plane_strain'
					                CTE='0.000000e+00'
					                C11='1.000000e+02'
					                C12='1.000000e+01'
					                C44='5.000000e+01'
					 rotation_angle_deg='1.500000e+01'/>
				</large_strain_material_2D>
			</large_strain_element_block>
		</updated_lagrangian>
	</element_list>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='10'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-12'
	      divergence_tolerance='1.000000e+01'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
</tahoe>
