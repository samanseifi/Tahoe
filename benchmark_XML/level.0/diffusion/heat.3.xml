<?xml version="1.0" encoding="UTF-8"?>
<tahoe geometry_file="../geometry/quad8.2x2.geom"
    output_format="ExodusII" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="10" output_inc="1" time_step="0.1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="temperature" integrator="trapezoid">
            <dof_labels>
                <String value="T"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" schedule="1" type="u" value="1.0"/>
        </field>
    </nodes>
    <element_list>
        <diffusion field_name="temperature">
            <quadrilateral/>
            <diffusion_element_nodal_output displacement="1" material_output="1"/>
            <diffusion_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <diffusion_material>
                    <linear_diffusion_material conductivity="2.0"
                        density="1.0" specific_heat="1.0"/>
                </diffusion_material>
            </diffusion_element_block>
        </diffusion>
    </element_list>
    <linear_solver>
        <profile_matrix/>
    </linear_solver>
</tahoe>
