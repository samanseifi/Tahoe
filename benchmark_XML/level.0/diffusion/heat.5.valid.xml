<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
    geometry_format='automatic'
      geometry_file='../geometry/quad8.2x2.geom'
      output_format='automatic'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	       scaling='linear'
	     init_time='0.000000e+00'
	     num_steps='10'
	    output_inc='1'
	 max_step_cuts='0'
	     time_step='1.000000e-01'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='0.000000e+00'/>
				<OrderedPair
				 x='1.000000e+00'
				 y='1.000000e+00'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='temperature'
		          solution_group='1'
		              integrator='trapezoid'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='T'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='1'
			     type='fixed'
			 schedule='1'
			    value='2.000000e+00'/>
		</field>
		<history_node_ID_list>
			<String
			 value='1'/>
		</history_node_ID_list>
	</nodes>
	<element_list>
		<diffusion
		 field_name='temperature'>
			<quadrilateral
			 num_ip='4'/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					       density='1.000000e+00'
					 specific_heat='1.000000e+00'
					  conductivity='1.000000e+00'/>
				</diffusion_material>
			</diffusion_element_block>
		</diffusion>
	</element_list>
	<linear_solver
	            print_eqnos='false'
	             check_code='no_check'
	 check_LHS_perturbation='1.000000e-08'>
		<profile_matrix/>
	</linear_solver>
</tahoe>
