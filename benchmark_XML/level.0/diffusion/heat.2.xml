<?xml version="1.0" encoding="UTF-8"?>
<tahoe geometry_file="../geometry/quad4.2x2.geom" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="1" output_inc="1" time_step="1.0">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="temperature">
            <dof_labels>
                <String value="T"/>
            </dof_labels>
            <mapped_nodes schedule="1">
                <Matrix_Nx2>
                    <Vector_2 v_1="0.1" v_2="0.2"/>
                </Matrix_Nx2>
                <mapped_node_ID_list>
                    <String value="1"/>
                    <String value="2"/>
                    <String value="3"/>
                    <String value="4"/>
                </mapped_node_ID_list>
            </mapped_nodes>
        </field>
    </nodes>
    <element_list>
        <diffusion field_name="temperature">
            <quadrilateral/>
            <diffusion_element_nodal_output displacement="1" material_output="1"/>
            <diffusion_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <diffusion_material>
                    <linear_diffusion_material conductivity="1.0"
                        density="1.0" specific_heat="1.0"/>
                </diffusion_material>
            </diffusion_element_block>
        </diffusion>
    </element_list>
    <linear_solver>
        <profile_matrix/>
    </linear_solver>
</tahoe>
