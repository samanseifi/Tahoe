<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" author="paklein" geometry_file="../geometry/square.1.geom" title="2D finite deformation RKPM with small supports">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="5"/>
            <kinematic_BC dof="2" node_ID="3" schedule="1" type="u" value="1.0"/>
            <kinematic_BC dof="1" node_ID="8"/>
        </field>
    </nodes>
    <element_list>
        <large_strain_meshfree field_name="displacement">
            <quadrilateral num_ip="16"/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <large_strain_StVenant_2D density="1.0">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                    </large_strain_StVenant_2D>
                </large_strain_material_2D>
            </large_strain_element_block>
            <meshfree_support_2D>
                <RKPM>
                    <cubic_spline_window support_scaling="2.02"/>
                </RKPM>
            </meshfree_support_2D>
            <meshfree_fracture_support/>
        </large_strain_meshfree>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10" divergence_tolerance="1.0e+01" max_iterations="10" rel_tolerance="1.0e-12"><profile_matrix/></nonlinear_solver>
</tahoe>
