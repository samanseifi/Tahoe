<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="../geometry/tet10.geom"
    title="tet4 test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="1" output_inc="1" time_step="1.0">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
				<String value="D_Z"/>
            </dof_labels>
            <mapped_nodes schedule="1">
                <Matrix_3x3 A_1_1="0.01" A_1_2="0.021" A_2_1="-0.03" A_2_2="-0.011" A_2_3="-0.011"/>
                <mapped_node_ID_list>
                    <String value="1"/>
                </mapped_node_ID_list>
            </mapped_nodes>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <tetrahedron num_ip='4'/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant density="1.0">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                    </small_strain_StVenant>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+10" max_iterations="2" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
