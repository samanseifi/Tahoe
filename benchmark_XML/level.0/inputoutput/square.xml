<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" geometry_file="./decomp/square.geom">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="central_difference">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="100"/>
            <kinematic_BC dof="2" node_ID="101"/>
            <kinematic_BC dof="1" node_ID="102"/>
        </field>
        <history_node_ID_list>
            <String value="102"/>
            <String value="101"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement" mass_type="lumped_mass">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <solid_element_element_output strain_energy="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="14"/>
                    <String value="11"/>
                    <String value="10"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <large_strain_StVenant_2D density="1.0">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                    </large_strain_StVenant_2D>
                </large_strain_material_2D>
            </large_strain_element_block>
        </updated_lagrangian>
        <updated_lagrangian field_name="displacement" mass_type="lumped_mass">
            <quadrilateral/>
            <natural_bc coordinate_system="global" schedule="1" side_set_ID="99">
                <DoubleList>
                    <Double value="0.0"/>
                    <Double value="-1.0"/>
                </DoubleList>
            </natural_bc>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <solid_element_element_output strain_energy="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="13"/>
                    <String value="12"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <large_strain_StVenant_2D density="1.0">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                    </large_strain_StVenant_2D>
                </large_strain_material_2D>
            </large_strain_element_block>
        </updated_lagrangian>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10" divergence_tolerance="1.0e+01" max_iterations="10" rel_tolerance="1.0e-12"><diagonal_matrix/></nonlinear_solver>
</tahoe>
