<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
        author='paklein'
 geometry_file='./geometry/square.geom'
 output_format='ExodusII'
         title='square region with prescribed temperature in corner'
      xmlns:x0='http://www.w3.org/2001/XMLSchema'>
	<time
	  num_steps='10'
	 output_inc='1'
	  time_step='0.1'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.0'
				 y='0.0'/>
				<OrderedPair
				 x='1.0'
				 y='1.0'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		     field_name='displacement'
		 solution_group='1'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
			</dof_labels>
			<kinematic_BC
			     dof='1'
			 node_ID='100'/>
			<kinematic_BC
			     dof='2'
			 node_ID='101'/>
			<kinematic_BC
			      dof='1'
			  node_ID='102'
			 schedule='1'
			     type='u'
			    value='1'/>
		</field>
		<field
		     field_name='electric_scalar_potential'
		 solution_group='2'>
			<dof_labels>
				<String
				 value='Psi'/>
			</dof_labels>
			<kinematic_BC
			     dof='1'
			 node_ID='102'
			    type='u'
			   value='1.0'/>
			<kinematic_BC
			     dof='1'
			 node_ID='100'
			    type='u'
			   value='0.0'/>
		</field>
	</nodes>
	<element_list>
		<diffusion
		 field_name='electric_scalar_potential'>
			<quadrilateral/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='14'/>
					<String
					 value='11'/>
					<String
					 value='10'/>
					<String
					 value='13'/>
					<String
					 value='12'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					  conductivity='1.0'
					       density='1.0'
					 specific_heat='1.0'/>
				</diffusion_material>
			</diffusion_element_block>
		</diffusion>
		<updated_lagrangian_Q1P0
		 field_name='displacement'>
			<quadrilateral/>
			<solid_element_nodal_output
			 displacements='1'
			        stress='1'/>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='14'/>
					<String
					 value='11'/>
					<String
					 value='10'/>
					<String
					 value='13'/>
					<String
					 value='12'/>
				</block_ID_list>
				<large_strain_material_2D>
					<large_strain_StVenant_2D
					     CTE='0.0'
					 density='1.0'>
						<E_and_nu
						 Poisson_ratio='0.25'
						 Young_modulus='100.0'/>
					</large_strain_StVenant_2D>
				</large_strain_material_2D>
			</large_strain_element_block>
		</updated_lagrangian_Q1P0>
	</element_list>
	<nonlinear_solver
	        abs_tolerance='1.0e-10'
	 divergence_tolerance='10.0'
	       max_iterations='10'
	        rel_tolerance='1.0e-12'>
		<profile_matrix/>
	</nonlinear_solver>
	<linear_solver>
		<profile_matrix/>
	</linear_solver>
	<solver_phases
	 max_loops='10'>
		<solver_phase
		      iterations='2'
		 pass_iterations='0'
		          solver='1'/>
		<solver_phase
		      iterations='2'
		 pass_iterations='0'
		          solver='2'/>
	</solver_phases>
</tahoe>
