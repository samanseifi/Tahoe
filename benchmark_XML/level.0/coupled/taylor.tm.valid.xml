<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
              title='2D Taylor impact example'
             author='paklein'
    geometry_format='automatic'
      geometry_file='./geometry/square.1x5.geom'
      output_format='automatic'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	       scaling='linear'
	     init_time='0.000000e+00'
	     num_steps='20'
	    output_inc='5'
	 max_step_cuts='0'
	     time_step='5.000000e-02'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='1.000000e+00'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='displacement'
		          solution_group='1'
		              integrator='nonlinear_HHT'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
			</dof_labels>
			<initial_condition
			 node_ID='104'
			     dof='2'
			    type='D_u'
			   value='-1.000000e-01'/>
			<wall_penalty
			            stiffness='5.000000e+02'
			 friction_coefficient='0.000000e+00'>
				<bc_initial_position>
					<Vector_2
					 v_1='0.000000e+00'
					 v_2='0.000000e+00'/>
				</bc_initial_position>
				<bc_velocity>
					<Vector_2
					 v_1='0.000000e+00'
					 v_2='0.000000e+00'/>
				</bc_velocity>
				<velocity_constant/>
				<node_ID_list>
					<String
					 value='100'/>
				</node_ID_list>
				<wall_normal>
					<Vector_2
					 v_1='0.000000e+00'
					 v_2='1.000000e+00'/>
				</wall_normal>
			</wall_penalty>
		</field>
		<field
		              field_name='temperature'
		          solution_group='2'
		              integrator='trapezoid'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='T'/>
			</dof_labels>
		</field>
	</nodes>
	<element_list>
		<updated_lagrangian
		     field_name='displacement'
		      mass_type='automatic'
		 eigenvalue_inc='0'>
			<quadrilateral
			 num_ip='4'/>
			<solid_element_nodal_output
			   displacements='1'
			          stress='1'
			 material_output='1'/>
			<solid_element_element_output
			 linear_momentum='1'/>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='10'/>
				</block_ID_list>
				<large_strain_material_2D>
					<Simo_J2_2D
					       density='1.000000e+00'
					 constraint_2D='plane_strain'
					           CTE='0.000000e+00'>
						<E_and_nu
						 Young_modulus='1.000000e+02'
						 Poisson_ratio='2.500000e-01'/>
						<linear_function
						 a='5.000000e-02'
						 b='2.500000e-01'/>
					</Simo_J2_2D>
				</large_strain_material_2D>
			</large_strain_element_block>
		</updated_lagrangian>
		<diffusion
		 field_name='temperature'>
			<quadrilateral
			 num_ip='4'/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='10'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					       density='1.000000e+00'
					 specific_heat='1.000000e+00'
					  conductivity='1.000000e+00'/>
				</diffusion_material>
			</diffusion_element_block>
		</diffusion>
	</element_list>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='10'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-12'
	      divergence_tolerance='1.000000e+01'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='10'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-12'
	      divergence_tolerance='1.000000e+01'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
	<solver_phases
	 max_loops='10'>
		<solver_phase
		          solver='1'
		      iterations='2'
		 pass_iterations='0'/>
		<solver_phase
		          solver='2'
		      iterations='2'
		 pass_iterations='0'/>
	</solver_phases>
</tahoe>
