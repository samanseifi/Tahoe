<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="./geometry/square.geom"
    output_format="ExodusII"
    title="square region with prescribed temperature in corner" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="100" output_inc="1" time_step="0.1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="100"/>
            <kinematic_BC dof="2" node_ID="101"/>
            <kinematic_BC dof="1" node_ID="102" schedule="1" type="u" value="0.1"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian_Q1P0 field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="14"/>
                    <String value="11"/>
                    <String value="10"/>
                    <String value="13"/>
                    <String value="12"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <RG_split_general density="1.0">
                        <rg_eq_potential>
                            <neo-hookean kappa="10000" mu="10"/>
                        </rg_eq_potential>
                    </RG_split_general>
                </large_strain_material_2D>
            </large_strain_element_block>
        </updated_lagrangian_Q1P0>
    </element_list>
    <linear_solver check_LHS_perturbation="1.0E-4">
        <diagonal_matrix/>
    </linear_solver>
</tahoe>
