<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="./geometry/80x4.geom"
    output_format="ExodusII"
    title="square region with prescribed temperature in corner" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="1000000" output_inc="1000" time_step="0.001">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1000000000.0" y="1000000000.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="4"/>
        </field>
        <field field_name="electric_scalar_potential" integrator="static">
            <dof_labels>
                <String value="Psi"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" schedule="1" type="u" value="0.01"/>
            <kinematic_BC dof="1" node_ID="3" type="fixed"/>
        </field>
    </nodes>
    <element_list>
        <dielectric_elastomer_Q1P02D
            electric_field_name="electric_scalar_potential"
            field_name="displacement" mass_type="lumped_mass">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                electric_field="1" electric_scalar_potential="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <Dielectric_Elastomer_Q1P02D Nrig="5.0"
                        density="1.0" epsilon="1.0" gamma="0.0"
                        lambda="1000.0" mu="1.0" t_0="10000.0"/>
                </large_strain_material_2D>
            </large_strain_element_block>
        </dielectric_elastomer_Q1P02D>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-7"
        check_LHS_perturbation="1.0E-8" check_code="no_check"
        divergence_tolerance="1.0e+10" max_iterations="50" rel_tolerance="1.0e-9">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
