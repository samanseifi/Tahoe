<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
              title='square region with prescribed temperature in corner'
             author='paklein'
    geometry_format='automatic'
      geometry_file='./geometry/8x8.geom'
      output_format='ExodusII'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	       scaling='linear'
	     init_time='0.000000e+00'
	     num_steps='10000000'
	    output_inc='1000'
	 max_step_cuts='0'
	     time_step='1.000000e-03'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='0.000000e+00'/>
				<OrderedPair
				 x='1.000000e+09'
				 y='1.000000e+09'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='displacement'
		          solution_group='1'
		              integrator='central_difference'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='2'
			      dof='1'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='1'
			      dof='2'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
		</field>
		<field
		              field_name='electric_scalar_potential'
		          solution_group='2'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='Psi'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='1'
			     type='u'
			 schedule='1'
			    value='1.000000e-02'/>
			<kinematic_BC
			  node_ID='3'
			      dof='1'
			     type='u'
			 schedule='0'
			    value='0.000000e+00'/>
		</field>
	</nodes>
	<element_list>
		<diffusion
		 field_name='electric_scalar_potential'>
			<quadrilateral
			 num_ip='4'/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					       density='1.000000e+00'
					 specific_heat='1.000000e+00'
					  conductivity='1.000000e+00'/>
				</diffusion_material>
			</diffusion_element_block>
		</diffusion>
		<updated_lagrangian_Q1P0
		     field_name='displacement'
		      mass_type='automatic'
		 eigenvalue_inc='0'>
			<quadrilateral
			 num_ip='4'/>
			<solid_element_nodal_output
			 displacements='1'
			        stress='1'/>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<large_strain_material_2D>
					<RG_split_general
					       density='1.000000e+00'
					 constraint_2D='none'
					           CTE='0.000000e+00'>
						<rg_eq_potential>
							<neo-hookean
							 kappa='1.000000e+03'
							    mu='1.000000e+00'/>
						</rg_eq_potential>
					</RG_split_general>
				</large_strain_material_2D>
			</large_strain_element_block>
		</updated_lagrangian_Q1P0>
	</element_list>
	<linear_solver
	            print_eqnos='false'
	             check_code='no_check'
	 check_LHS_perturbation='1.000000e-08'>
		<SPOOLES_matrix
		    message_level='silent'
		  enable_pivoting='true'
		 always_symmetric='false'/>
	</linear_solver>
	<linear_solver
	            print_eqnos='false'
	             check_code='no_check'
	 check_LHS_perturbation='1.000000e-08'>
		<SPOOLES_matrix
		    message_level='silent'
		  enable_pivoting='true'
		 always_symmetric='false'/>
	</linear_solver>
	<solver_phases
	 max_loops='10'>
		<solver_phase
		          solver='1'
		      iterations='2'
		 pass_iterations='0'/>
		<solver_phase
		          solver='2'
		      iterations='2'
		 pass_iterations='0'/>
	</solver_phases>
</tahoe>
