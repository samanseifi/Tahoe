<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" author="paklein" geometry_file="../geometry/rect.5x1.geom" title="2D Q1P0, axisymmetric test">
    <time num_steps="1" output_inc="1" time_step="1.0">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian_Q1P0_axi field_name="displacement">
            <quadrilateral/>
            <natural_bc coordinate_system="global" schedule="1" side_set_ID="1">
                <DoubleList>
                    <Double value="0.0"/>
                    <Double value="-1.0"/>
                </DoubleList>
            </natural_bc>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_axi_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <large_strain_StVenant density="1.0">
                        <E_and_nu Poisson_ratio="0.33" Young_modulus="100.0"/>
                    </large_strain_StVenant>
                </large_strain_material_3D>
            </large_strain_axi_element_block>
        </updated_lagrangian_Q1P0_axi>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10" divergence_tolerance="1.0e+01" max_iterations="5" rel_tolerance="1.0e-12"><profile_matrix/></nonlinear_solver>
</tahoe>
