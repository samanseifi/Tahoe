<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="../geometry/cube.0.geom"
    title="3D spring elements" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <linear_function a="1.0" b="0.0"/>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <kinematic_BC dof="3" node_ID="2"/>
            <kinematic_BC dof="1" node_ID="3" schedule="1" type="u" value="0.1"/>
        </field>
        <history_node_ID_list>
            <String value="3"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <spring_element field_name="displacement">
            <spring_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <spring_linear mass="1.0" stiffness="1.0"/>
            </spring_element_block>
        </spring_element>
    </element_list>
    <PCG_solver abs_tolerance="1.0e-10" divergence_tolerance="1.0e+10"
        max_iterations="100" rel_tolerance="1.0e-10" restart="100">
        <profile_matrix/>
    </PCG_solver>
</tahoe>
