<?xml version="1.0" encoding="UTF-8"?>
<tahoe_bridging author="paklein" geometry_file="./geometry/grid.geom"
	logging="moderate"
    title="continuum solver" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="1" output_inc="1" time_step="1.0"/>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <hex_2D density="1.0" shells="5">
                        <CB_lattice_Hex>
                            <HEX2D_natural/>
                        </CB_lattice_Hex>
                        <Lennard_Jones cut_off_distance="2.63"
                            energy_scaling="0.5678950796453064"
                            length_scaling="2.62311730030557" mass="196.97 "/>
                    </hex_2D>
                </large_strain_material_2D>
            </large_strain_element_block>
        </updated_lagrangian>
        <meshfree_bridging field_name="displacement" solid_element_group="1">
            <RKPM cross_terms="true">
                <cubic_spline_window support_scaling="1.25"/>
            </RKPM>
        </meshfree_bridging>
    </element_list>
    <PCG_solver abs_tolerance="1.0e-12" divergence_tolerance="1.0e+06"
        line_search_iterations="10" line_search_tolerance="0.1"
        max_iterations="100000" max_step="2.5" quick_solve_iter="500"
        rel_tolerance="1.0e-12" restart="50">
        <diagonal_matrix/>
    </PCG_solver>
</tahoe_bridging>
