<?xml version="1.0" encoding="UTF-8"?>
<tahoe_multi atom_input="lattice.nocross.xml"
    bridging_field="displacement" coarse_to_coarse="false"
    coarse_to_fine="true" continuum_input="continuum.mf.nocross.xml"
    fine_to_coarse="true" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <ghost_atom_ID_list>
        <String value="6"/>
    </ghost_atom_ID_list>
    <by-bond_multiplier bind_to_1.0="0.0"
        constraint_regularization="10.0" density_nip="default"
        init_bound_width="1.0" smoothing="1.0e-06"/>
    <PCG_solver abs_tolerance="1.0e-12" divergence_tolerance="1.0e+15"
        line_search_iterations="10" line_search_tolerance="0.1"
        max_iterations="1000" max_step="2.5" quick_solve_iter="500"
        rel_tolerance="1.0e-11" restart="100">
        <diagonal_matrix/>
    </PCG_solver>
</tahoe_multi>
