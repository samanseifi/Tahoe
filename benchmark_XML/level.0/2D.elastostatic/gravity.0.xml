<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" geometry_file="../geometry/quad4.2x2.geom" xsi:noNamespaceSchemaLocation="file:///Users/paklein/Code/tahoe/projects/tahoe.xsd">    
	<time num_steps="1" output_inc="1" time_step="1.0">        
		<schedule_function>            
			<piecewise_linear>                
				<OrderedPair x="0.0" y="0.0"/>                
				<OrderedPair x="1.0" y="1.0"/>            
			</piecewise_linear>        
		</schedule_function>    
	</time>    
	<nodes>        
		<field field_name="displacement">            
			<dof_labels>                
				<String value="D_X"/>                
				<String value="D_Y"/>            
			</dof_labels>            
			<kinematic_BC dof="2" node_ID="1"/>            
			<kinematic_BC dof="1" node_ID="2"/>            
			<kinematic_BC dof="1" node_ID="4"/>        
		</field>    
	</nodes>    
	<element_list>        
		<small_strain field_name="displacement">            
			<quadrilateral/>            
			<body_force schedule="1">                
				<Double value="0.0"/>                
				<Double value="-10.0"/>            
			</body_force>            
			<solid_element_nodal_output displacements="1" stress="1"/>            
			<small_strain_element_block>                
				<block_ID_list>                    
					<String value="1"/>                
				</block_ID_list>                
				<small_strain_material_2D>                    
					<small_strain_StVenant_2D density="1.0">                        
						<E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>                    
					</small_strain_StVenant_2D>                
				</small_strain_material_2D>            
			</small_strain_element_block>        
		</small_strain>    
	</element_list>    
	<nonlinear_solver abs_tolerance="1.0e-10" rel_tolerance="1.0e-12" divergence_tolerance="1.0e+01" max_iterations="10"><profile_matrix/></nonlinear_solver>
</tahoe>
