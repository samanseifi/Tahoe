<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" author="paklein" geometry_file="geometry/hex_lattice.5x5.geom" title="small hex lattice with damping">
    <time num_steps="100" output_inc="25" time_step="0.00125">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="1.0"/>
                <OrderedPair x="50.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="1.0"/>
                <OrderedPair x="50.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="Gear6">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <scaled_velocity mass="1.0" seed="12345">
                <scale_as_IC temperature="28.0"/>
                <pick_nodes_by_list selection="exclude_these">
                    <node_ID_list>
                        <String value="1"/>
                        <String value="2"/>
                    </node_ID_list>
                </pick_nodes_by_list>
            </scaled_velocity>
        </field>
    </nodes>
    <element_list>
        <particle_pair field_name="displacement" lattice_parameter="1.0" max_neighbor_distance="1.1" re-neighbor_displacement="1.0">
            <Gauss_isokinetic beta="1.0">
                <thermostat_temperature schedule="2" value="10.0"/>
                <pick_by_list selection="exclude_these">
                    <node_ID_list>
                        <String value="1"/>
                        <String value="2"/>
                    </node_ID_list>
                </pick_by_list>
            </Gauss_isokinetic>
            <particle_type label="A">
                <node_ID_list>
                    <String value="3"/>
                </node_ID_list>
            </particle_type>
            <pair_particle_interaction label_1="A" label_2="A">
                <harmonic mass="1.0" rest_length="1.0" stiffness="1.0"/>
            </pair_particle_interaction>
            <particle_pair_output centrosymmetry="1" displacement="1"
                kinetic_energy="1" potential_energy="1" slip_vector="1"
                strain="1" stress="1"/>
        </particle_pair>
    </element_list>
    <linear_solver><diagonal_matrix/></linear_solver>
</tahoe>
