<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" author="paklein" geometry_file="../geometry/two_cubes.4.geom" title="two cubes in contact">
    <time max_step_cuts="2" num_steps="30" output_inc="10" time_step="0.1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="1.0" y="0.0"/>
                <OrderedPair x="3.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="4"/>
            <kinematic_BC dof="2" node_ID="4" schedule="2" type="u" value="4.0"/>
            <kinematic_BC dof="3" node_ID="4" schedule="1" type="u" value="-0.2"/>
        </field>
        <history_node_ID_list>
            <String value="4"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <Simo_isotropic density="1.0">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                    </Simo_isotropic>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian>
        <contact_drag_3D_penalty drag_traction="0.2" field_name="displacement" gap_tolerance="0.1" penalty_stiffness="2.0e+02" slip_tolerance="0.1">
            <contact_surface>
                <surface_side_set side_set_ID="1"/>
            </contact_surface>
            <contact_surface>
                <surface_side_set side_set_ID="2"/>
            </contact_surface>
            <contact_nodes>
                <node_ID_list>
                    <String value="3"/>
                </node_ID_list>
            </contact_nodes>
        </contact_drag_3D_penalty>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10" divergence_tolerance="1.0e+03" max_iterations="15" rel_tolerance="1.0e-12"><profile_matrix/></nonlinear_solver>
</tahoe>
