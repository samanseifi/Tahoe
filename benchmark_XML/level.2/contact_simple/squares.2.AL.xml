<?xml version="1.0"?>
<tahoe xmlns:x0="http://www.w3.org/2001/XMLSchema" author="paklein" echo_input="true" geometry_file="../geometry/two_squares.1.geom" title="two squares in contact">
    <time num_steps="5" output_inc="5" time_step="0.02">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="4"/>
            <kinematic_BC dof="2" node_ID="4" schedule="1" type="u" value="-0.2"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <Simo_isotropic_2D density="1.0">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                    </Simo_isotropic_2D>
                </large_strain_material_2D>
            </large_strain_element_block>
        </updated_lagrangian>
        <contact_2D_multiplier field_name="displacement" regularization="50.0">
            <contact_surface>
                <surface_side_set side_set_ID="1"/>
            </contact_surface>
            <contact_surface>
                <surface_side_set side_set_ID="2"/>
            </contact_surface>
            <contact_nodes>
                <node_ID_list>
                    <String value="3"/>
                </node_ID_list>
            </contact_nodes>
        </contact_2D_multiplier>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10" divergence_tolerance="1.0e+03" max_iterations="5" rel_tolerance="1.0e-12"><profile_matrix/></nonlinear_solver>
</tahoe>
